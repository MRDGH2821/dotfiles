{
  "goTemplateBracketSpacing": true,
  "overrides": [
    {
      "files": ["*.jsonc", "*.json"],
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    }
  ],
  "plugins": ["prettier-plugin-go-template", "prettier-plugin-toml"],
  "tabWidth": 2,
  "useTabs": false
}
